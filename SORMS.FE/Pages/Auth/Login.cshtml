@page
@model 
@{
    ViewData["Title"] = "Đăng nhập";
}

<h2>Đăng nhập</h2>

<form method="post">
    <div>
        <label>Email:</label>
        <input asp-for="LoginDto.Email" />
    </div>
    <div>
        <label>Mật khẩu:</label>
        <input asp-for="LoginDto.Password" type="password" />
    </div>
    <button type="submit">Đăng nhập</button>
</form>

@if (Model.Message != null)
{
    <p>@Model.Message</p>
}
